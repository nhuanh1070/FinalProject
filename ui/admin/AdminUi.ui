            QSlider::groove:horizontal {
                border: 1px solid #999;
                background: #ECECEC;   /* màu nền rãnh */
                height: 8px;           /* chiều cao của rãnh */
                border-radius: 4px;    /* bo góc rãnh */
            }
            QSlider::handle:horizontal {
                background: #E0E0E0;   /* màu nền nút trượt */
                border: 1px solid #777;
                width: 16px;           /* độ rộng nút trượt */
                height: 16px;          /* độ cao nút trượt (chủ yếu là bo tròn) */
                margin: -5px 0;        /* canh vị trí nút trượt nằm giữa rãnh */
                border-radius: 8px;    /* bo tròn nút trượt */
            }
            QSlider::handle:horizontal:hover {
                background: #D6D6D6;   /* màu nút trượt khi hover */
            }
            QSlider::sub-page:horizontal {
                background: #0078D7;   /* màu phần rãnh đã trượt qua */
                border-radius: 4px;
            }
            QSlider::add-page:horizontal {
                background: #ECECEC;   /* màu phần rãnh chưa trượt tới */
                border-radius: 4px;
            }</string>
